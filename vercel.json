{
   "version": 2,
   "builds": [
     {
       "src": "api/index.js",
       "use": "@vercel/node"
     },
     {
       "src": "dashboard/dist/**",
       "use": "@vercel/static"
     }
   ],
   "routes": [
     {
       "src": "/graphql",
       "dest": "api/index.js"
     },
     {
       "src": "/projects/(.*)",
       "dest": "api/index.js"
     },
     {
       "src": "/(.*)",
       "dest": "dashboard/dist/$1"
     }
   ]
 }
 