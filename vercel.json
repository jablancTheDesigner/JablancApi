{
    "version": 2,
    "builds": [
      {
         "src": "dashboard",
         "use": "@vercel/vite",
         "config": { "includeFiles": ["*"] }
     },
     {
        "src": "api/index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["*"] }
    }
    ],
    "routes": [
      {
         "src": "/(.*)",
         "dest": "api/index.js"
      }
    ]
   }